        <PackageReference Include="Grpc.Tools" Version="2.67.0" PrivateAssets="All"/>
        <PackageReference Include="GrpcDotNetNamedPipes" Version="3.0.0" />
        <PackageReference Include="Sentry.Serilog" Version="4.12.1"/>
        <PackageReference Include="Serilog.Extensions.Logging" Version="8.0.0"/>
        <PackageReference Include="Serilog.Sinks.Console" Version="6.0.0"/>
        <PackageReference Include="Serilog.Sinks.EventLog" Version="4.0.0"/>
